    line-height: 45px;
    padding: 0px 20px 0px;
    /* text-align: center; */
}

.brand-logo a span {
    display: block;
    /* vertical-align: middle; */
    margin-left: 0px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.brand-logo a img {
    display: block;
    margin: 0 auto;
    max-width: 70px;
}

.left-side-bar {
    position: fixed;
    left: 0;
    top: 0;
    width: 250px;
    height: 100vh;
